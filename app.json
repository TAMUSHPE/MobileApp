{
  "expo": {
    "name": "TAMU SHPE",
    "slug": "TAMU-SHPE",
    "version": "1.1.0",
    "owner": "tamu-shpe",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "TAMU SHPE needs to access your photo library to select a profile picture."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Access access to files in iCloud"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "TAMU SHPE needs access to your camera to scan QRCode",
          "microphonePermission": "Allow TAMUSHPE to access your microphone",
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "TAMU SHPE needs access to your location to show your position on the map."
        }
      ],
      "expo-web-browser"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "tamu-shpe",
    "android": {
      "package": "com.tamu.shpe",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "versionCode": 152,
      "userInterfaceStyle": "automatic",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB1aZFHZCIN0RdvpzfWQflkHpiyGd2Qfo8"
        }
      }
    },
    "ios": {
      "bundleIdentifier": "com.tamu.shpe",
      "userInterfaceStyle": "automatic",
      "buildNumber": "5",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "aps-environment": "development"
      }
    },
    "extra": {
      "eas": {
        "projectId": "5b75342a-5739-4b5b-838f-89ff1fe3979d"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/5b75342a-5739-4b5b-838f-89ff1fe3979d"
    }
  }
}
