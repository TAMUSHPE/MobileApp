{
  "expo": {
    "name": "TAMU SHPE",
    "slug": "TAMU-SHPE",
    "version": "1.0.1",
    "owner": "tamu-shpe",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "TAMU SHPE needs to access your photo library to select a profile picture."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Access access to files in iCloud"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "TAMU SHPE needs to access the camera to scan QR codes."
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "tamu-shpe",
    "android": {
      "package": "com.tamu.shpe",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "versionCode": 41,
      "userInterfaceStyle": "automatic"
    },
    "ios": {
      "bundleIdentifier": "com.tamu.shpe",
      "buildNumber": "2",
      "userInterfaceStyle": "automatic"
    },
    "extra": {
      "eas": {
        "projectId": "5b75342a-5739-4b5b-838f-89ff1fe3979d"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/5b75342a-5739-4b5b-838f-89ff1fe3979d"
    }
  }
}
